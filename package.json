{
  "name": "lms-export-to-ladok-2",
  "version": "1.0.0",
  "engines": {
    "node": ">=14"
  },
  "description": "Send results of an assignment in Canvas LMS as module in Ladok",
  "main": "app.js",
  "scripts": {
    "test:lint": "eslint *.js client lib server",
    "test": "prettier --check . && npm run test:lint",
    "test:browser": "npx cypress open --project e2e",
    "test:browser-docker": "docker-compose -f docker-compose-integration-tests.yml up --abort-on-container-exit --build",
    "format": "prettier --write .",
    "start": "node .",
    "build": "NODE_ENV=production webpack",
    "debug": "NODE_ENV=development node --nolazy app.js | pino-pretty -e options,response -o '{msg}' --ignore options,pid,hostname,app,req_id --singleLine --translateTime 'HH:MM:ss.l'",
    "dev": "NODE_ENV=development node . | pino-pretty -e options,response -o '{msg}' --ignore options,pid,hostname,app,req_id --singleLine --translateTime 'HH:MM:ss.l'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KTH/lms-export-to-ladok-2.git"
  },
  "keywords": [],
  "author": "KTH Royal Institute of Technology",
  "license": "MIT",
  "dependencies": {
    "@kth/canvas-api": "^2.1.2",
    "@kth/reqvars": "^2.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.5",
    "express-promise-router": "^3.0.3",
    "got": "^9.6.0",
    "kth-style": "^6.0.21",
    "memoizee": "^0.4.14",
    "mongodb": "^3.6.2",
    "react-datepicker": "^3.3.0",
    "react-hot-loader": "^4.13.0",
    "skog": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@kth/eslint-config-kth": "^1.0.10",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^5.2.6",
    "eslint": "^7.30.0",
    "inquirer": "^8.1.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.6",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^8.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.1"
  }
}
